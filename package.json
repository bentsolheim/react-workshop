{
  "name": "react-workshop",
  "version": "1.0.0",
  "description": "react-workshop",
  "main": "index.js",
  "scripts": {
    "clean:dist": "node ./scripts/clean-dist.js",
    "build:webpack": "webpack --config ./webpack/webpack.config.prod.js",
    "build": "npm run clean:dist && npm run build:webpack",
    "start": "webpack-dev-server --config ./webpack/webpack.config.dev.js --progress --colors",
    "test:watch": "jest --watch",
    "test": "jest",
    "nsp": "nsp check",
    "eslint": "eslint --color src",
    "eslint:watch": "esw --color --watch src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudfoss/react-workshop.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/rudfoss/react-workshop/issues"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>src/setupTests.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "homepage": "https://github.com/rudfoss/react-workshop#readme",
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.4",
    "extract-css-chunks-webpack-plugin": "^2.0.18",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "nsp": "^3.2.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "react-hot-loader": "^4.0.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "history": "^4.7.2",
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.2.1"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-1"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "rules": {
      "no-var": 2,
      "no-unused-vars": [
        "warn"
      ],
      "no-extra-boolean-cast": 0,
      "eqeqeq": [
        "warn",
        "smart"
      ],
      "no-console": 0,
      "semi": [
        1,
        "always"
      ],
      "no-trailing-spaces": 0,
      "no-debugger": 2,
      "no-lone-blocks": 2,
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "import/no-unresolved": 0
    }
  },
  "eslintIgnore": [
    "**/*.test.js"
  ],
  "browserslist": [
    "last 2 versions"
  ]
}
